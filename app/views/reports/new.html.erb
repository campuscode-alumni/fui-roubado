<h1>Relatar roubo</h1>
<%= simple_form_for @report do |f| %>
  <%= f.input :adress, label: 'Endereço' %>
  <%= f.input :ocurrance_date, htlm: true %>
  <%= f.input :description, label: 'Descrição' %>
  <%= f.input :police_report, label: 'Boletim de ocorrência', as: :file %>
  <%= f.assossiation :items, as: :check_boxes%>
  <%= f.submit 'Reportar' %>
<% end %>